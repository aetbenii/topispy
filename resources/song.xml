<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "https://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="logik.Song" table="SONG">

        <id name="songid" type="int">

            <column name="SONGID" />
            <generator class="increment"/>

        </id>

        <property name="song_name" type="java.lang.String">
            <column name="SONG_NAME" />
        </property>

        <property name="album" type="java.lang.String">
            <column name="ALBUM" />
        </property>

        <set name="artist" table="artist_song" fetch="select">
                <key column="songid" />
            <many-to-many class="logik.Artist" column="artistid" />

        </set>


    </class>
</hibernate-mapping>

